class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int f=0;
        int l=nums.size()-1;
        
        while(f<=l){
            int mid=(f+l)/2;
            if (target==nums[mid]){
                return mid;
            }
            else if(target>nums[mid]){
                f=mid+1;
            }
            else if (target<nums[mid]){
                l=mid-1;
            }
        }
        return f;
    }
};
